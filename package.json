{
  "name": "miobot",
  "version": "0.0.0",
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "main": "src/server.js",
  "scripts": {
    "build": "run-p build:*",
    "build:web": "yarn --cwd src/web build",
    "dev": "run-p dev:*",
    "dev:server": "nodemon -w src --exec node src/server.js",
    "dev:web": "PORT=5000 BROWSER=none yarn --cwd src/web start",
    "heroku-prebuild": "yarn --cwd src/web install",
    "job": "node src/job.js",
    "ngrok": "ngrok http 3000",
    "ship": "git push heroku master",
    "start": "node src/server.js"
  },
  "dependencies": {
    "date-fns": "^2.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.13",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "styled-components": "^4.3.2",
    "telegraf": "^3.32.0",
    "telegraf-session-redis": "^5.1.0"
  },
  "license": "MIT"
}
